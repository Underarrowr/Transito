/**
 * Description of Cl_Infraccion
 *
 * @author LC13005
 */
class Cl_Infraccion {
    private $idInfraccion;
    private $descripcion;
    private $personaIdPersona;
    private $apelada;
    private $monto;
    private $fiscalizadorIdFiscalizador;
    
    function __construct() {
        
    }
    function getIdInfraccion() {
        return $this->idInfraccion;
    }

    function getDescripcion() {
        return $this->descripcion;
    }

    function getPersonaIdPersona() {
        return $this->personaIdPersona;
    }

    function getApelada() {
        return $this->apelada;
    }

    function getMonto() {
        return $this->monto;
    }

    function getFiscalizadorIdFiscalizador() {
        return $this->fiscalizadorIdFiscalizador;
    }

    function setIdInfraccion($idInfraccion) {
        $this->idInfraccion = $idInfraccion;
    }

    function setDescripcion($descripcion) {
        $this->descripcion = $descripcion;
    }

    function setPersonaIdPersona($personaIdPersona) {
        $this->personaIdPersona = $personaIdPersona;
    }

    function setApelada($apelada) {
        $this->apelada = $apelada;
    }

    function setMonto($monto) {
        $this->monto = $monto;
    }

    function setFiscalizadorIdFiscalizador($fiscalizadorIdFiscalizador) {
        $this->fiscalizadorIdFiscalizador = $fiscalizadorIdFiscalizador;
    }



}
